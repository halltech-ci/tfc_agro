<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <template id="report_check_template">
        <t t-call="web.html_container">
            <t t-set="data_report_margin_top" t-value="12"/>
            <t t-set="data_report_header_spacing" t-value="9"/>
            <t t-set="data_report_dpi" t-value="110"/>
            <t t-foreach="docs" t-as="o">
                <t t-call="web.internal_layout">
                  <div class="page">
                      <h2>Check Status Reports</h2>
                      <div class="row">
                            <div class="col-3">
                                <strong>Company:</strong>
                                <p t-esc="res_company.name"/>
                            </div>
                        </div>
                      <div class="">
                        <t t-if="data['form']['date_from']"><strong>Date from :</strong><span t-esc="data['form']['date_from']"/></t>
                        <t t-if="data['form']['date_to']"><strong><span> Date to :</span></strong> <span t-esc="data['form']['date_to']"/></t>
                      </div>
                   </div><!-- End Page-->
                </t>
            </t>
        </t>
    </template>
    </data>
</odoo>
